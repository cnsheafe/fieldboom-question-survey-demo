{
  "name": "react-static-example-apollo-redux",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "build": "react-static build",
    "test": "jest",
    "serve": "serve dist -p 3000",
    "build-css": "node-sass-chokidar src/sass -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass -o src/styles --watch --recursive"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transform": {
      "\\.js$": "babel-jest",
      "\\.css$": "<rootDir>/style-mock.js",
      "\\.png$": "<rootDir>/style-mock.js"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "immutable": "^3.8.2",
    "json-loader": "^0.5.7",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-static": "^4.3.4",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "sass-loader": "^6.0.6",
    "serve": "^6.1.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  }
}
